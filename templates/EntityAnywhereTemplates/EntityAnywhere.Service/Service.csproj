<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Rhyous.WebFramework.Services</RootNamespace>
    <AssemblyName>Rhyous.WebFramework.Services.$Entity$</AssemblyName>
    <TargetFramework>net48</TargetFramework>
    <AssemblyTitle>Services.Product</AssemblyTitle>
    <Product>Services.Product</Product>
    <Copyright>Copyright Â©  2016</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="SET proj=$(ProjectName)
SET entity=%proj:*.=%
ECHO entity = %entity%
SET copyToDir=&quot;$(SolutionDir)WebServices\WebServices.Main\Plugins\Services\%entity%&quot;
ECHO %25copyToDir%25
IF NOT EXIST %25copyToDir%25 MKDIR %25copyToDir%25
COPY /Y &quot;$(TargetPath)&quot; %25copyToDir%25
COPY /Y &quot;$(TargetDir)$(TargetName).pdb&quot; %25copyToDir%25

SET dllDir=&quot;%copyToDir%\bin&quot;
ECHO %dllDir%
IF NOT EXIST %dllDir% MKDIR %dllDir%
FOR %%X in (&quot;$(Targetdir)*.dll&quot;) DO (
IF NOT &quot;%%X&quot;==&quot;$(TargetPath)&quot; COPY /Y &quot;%%X&quot; %dllDir%
)
FOR %%X in (&quot;$(Targetdir)*.pdb&quot;) DO (
IF NOT &quot;%%X&quot;==&quot;$(TargetDir)$(TargetName).pdb&quot; COPY /Y &quot;%%X&quot; %dllDir%
" />
  </Target>
  <Import Project="..\..\Imports\netstandard\Rhyous.StringLibrary.Import.csproj" />
  <ItemGroup>
    <ProjectReference Include="..\..\Entities\Entities.Product\Entities.$Entity$.csproj" />
    <ProjectReference Include="..\..\Interfaces\Interfaces.Common\Interfaces.Common.csproj" />
    <ProjectReference Include="..\..\Interfaces\Interfaces.Product\Interfaces.$Entity$.csproj" />
    <ProjectReference Include="..\..\Interfaces\Interfaces.Services\Interfaces.Services.csproj" />
    <ProjectReference Include="..\Services.Common\Services.Common.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Rhyous.Collections" Version="1.0.13" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.7.0" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>