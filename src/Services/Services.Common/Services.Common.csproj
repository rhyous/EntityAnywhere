<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Rhyous.EntityAnywhere.Services</RootNamespace>
    <AssemblyName>Rhyous.EntityAnywhere.Services.Common</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyTitle>Rhyous.EntityAnywhere.Services.Common</AssemblyTitle>
    <Product>Rhyous.EntityAnywhere.Services.Common</Product>
    <Copyright>Copyright © Rhyous Publishing LLC 2017</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <Import Project="..\..\Imports\netstandard\Rhyous.Odata.All.Import.csproj" />
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="SET proj=$(ProjectName)&#xD;&#xA;SET entity=%25proj:*.=%25&#xD;&#xA;ECHO entity = %25entity%25&#xD;&#xA;SET copyToDir=&quot;$(SolutionDir)WebServices\WebServices.Main\Plugins\Services\%25entity%25&quot;&#xD;&#xA;ECHO %25copyToDir%25&#xD;&#xA;IF NOT EXIST %25copyToDir%25 MKDIR %25copyToDir%25&#xD;&#xA;COPY /Y &quot;$(TargetPath)&quot; %25copyToDir%25&#xD;&#xA;COPY /Y &quot;$(TargetDir)$(TargetName).pdb&quot; %25copyToDir%25&#xD;&#xA;&#xD;&#xA;REM Uncomment to copy a dependency&#xD;&#xA;REM SET dllDir=%25copyToDir%25\bin&#xD;&#xA;REM ECHO %25dllDir%25&#xD;&#xA;REM IF NOT EXIST %25dllDir%25 MKDIR %25dllDir%25&#xD;&#xA;REM COPY /Y &quot;$(TargetDir)Dependency.dll&quot; %25dllDir%25&#xD;&#xA;" />
  </Target>
  <ItemGroup>
    <ProjectReference Include="..\..\Clients\Clients2.Common\Clients2.Common.csproj" />
    <ProjectReference Include="..\..\Entities\Entities.Entity\Entities.Entity.csproj" />
    <ProjectReference Include="..\..\Interfaces\Interfaces.Common\Interfaces.Common.csproj" />
    <ProjectReference Include="..\..\Interfaces\Interfaces.Services\Interfaces.Services.csproj" />
    <ProjectReference Include="..\..\Misc\Attributes.Common\Attributes.Common.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="LinqKit.Core" Version="1.1.17" />
    <PackageReference Include="Rhyous.SimplePluginLoader" Version="1.5.30" />
    <PackageReference Include="Rhyous.SimplePluginLoader.Autofac" Version="1.5.30" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="5.0.0" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>
</Project>